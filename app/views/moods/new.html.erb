<%= form_for(@mood) do |f| %>
	<div class="field">
		<p>Feeling</p>
		<%=f.text_field :feeling%>
	</div>
	<div class="field">
		<p>Adjective</p>
		<%=f.text_field :adjective%>
	</div>
	<div class="field">
		<p>user</p>
		<%=f.collection_select :user_id, User.all, :id, :name%>
	</div>
	<div class="actions">
		<%=f.submit%>
	</div>
<% end %>