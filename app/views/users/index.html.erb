<table>
	<tr>
		<td>name</td>
		<td>email</td>
		<td>password</td>
	</tr>
	<% @users.each do |user| %>
	<tr>
		<td><%=link_to user.name, user_path(user.id)%></td>
		<td><%=user.email%></td>
		<td><%=user.password%></td>
		<td><%=link_to "edit user", edit_user_path(user.id)%></td>
		<td><%=link_to "Delete user", user, method: :delete%></td>
	</tr>
	<% end %>
</table>